{
  "name": "@katjes733/react-chatbot-kit",
  "version": "1.0.0",
  "description": "Fully customizable chatbot kit for react.",
  "keywords": [
    "react",
    "react-component",
    "chatbot",
    "chatbot-kit",
    "chat-ui",
    "conversational-ui",
    "chatbot-framework"
  ],
  "main": "build/index.js",
  "types": "build/src/index.d.ts",
  "dependencies": {
    "react-conditionally-render": "^1.0.2",
    "react-textarea-autosize": "^8.5.9"
  },
  "scripts": {
    "test": "jest",
    "test:coverage": "jest --coverage",
    "build": "rimraf build && webpack --mode=production",
    "build-watch": "webpack --watch",
    "prepare": "yarn run build",
    "lint": "eslint \"src/**/*.{js,jsx,ts,tsx}\"",
    "lint:fix": "eslint \"src/**/*.{js,jsx,ts,tsx}\" --fix",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "verify": "yarn typecheck && yarn format:check && yarn && yarn test:coverage",
    "typecheck": "tsc --noEmit"
  },
  "author": {
    "name": "katjes733",
    "email": "katjes733@gmx.net"
  },
  "devDependencies": {
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@types/jest": "^30.0.0",
    "@types/react": "18",
    "@types/react-dom": "18",
    "@typescript-eslint/eslint-plugin": "^8.53.1",
    "@typescript-eslint/parser": "^8.53.1",
    "css-loader": "^7.1.2",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-prettier": "^5.5.5",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "mini-css-extract-plugin": "^2.10.0",
    "prettier": "^3.8.1",
    "react": "18",
    "react-dom": "18",
    "react-svg-loader": "^3.0.3",
    "rimraf": "^6.1.2",
    "style-loader": "^4.0.0",
    "ts-jest": "^29.4.6",
    "ts-loader": "^9.5.4",
    "typescript": "^5.9.3",
    "webpack": "^5.104.1",
    "webpack-cli": "^6.0.1",
    "webpack-node-externals": "^3.0.0"
  },
  "license": "MIT",
  "files": [
    "build",
    "src"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/katjes733/react-chatbot-kit"
  }
}
