{
  "name": "@katjes733/react-chatbot-kit",
  "version": "1.0.0",
  "description": "Fully customizable chatbot kit for react.",
  "keywords": [
    "react",
    "react-component",
    "chatbot",
    "chatbot-kit",
    "chat-ui",
    "conversational-ui",
    "chatbot-framework"
  ],
  "main": "build/index.js",
  "types": "build/src/index.d.ts",
  "dependencies": {
    "react-conditionally-render": "^1.0.2",
    "react-textarea-autosize": "^8.5.9"
  },
  "scripts": {
    "test": "jest",
    "test:coverage": "jest --coverage",
    "build": "rimraf build && webpack --mode=production",
    "build-watch": "webpack --watch",
    "prepare": "yarn run build",
    "lint": "eslint \"src/**/*.{js,jsx,ts,tsx}\"",
    "lint:fix": "eslint \"src/**/*.{js,jsx,ts,tsx}\" --fix",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "verify": "yarn run format:check && yarn test:coverage"
  },
  "author": {
    "name": "katjes733",
    "email": "katjes733@gmx.net"
  },
  "devDependencies": {
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.1",
    "@types/jest": "^30.0.0",
    "@types/react": "18",
    "@types/react-dom": "18",
    "@typescript-eslint/eslint-plugin": "^6.11.0",
    "@typescript-eslint/parser": "^6.11.0",
    "css-loader": "^3.5.1",
    "eslint": "^8.44.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-prettier": "^5.1.0",
    "eslint-plugin-react": "^7.33.0",
    "eslint-plugin-react-hooks": "^7.0.1",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "mini-css-extract-plugin": "^1.4.0",
    "prettier": "^2.8.8",
    "react": "18",
    "react-dom": "18",
    "react-svg-loader": "^3.0.3",
    "rimraf": "^5.0.0",
    "style-loader": "^1.1.3",
    "ts-jest": "^29.4.6",
    "ts-loader": "^9.2.5",
    "typescript": "^4.4.3",
    "webpack": "^5.104.1",
    "webpack-cli": "^6.0.1",
    "webpack-node-externals": "^2.5.2"
  },
  "license": "MIT",
  "files": [
    "build",
    "src"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/katjes733/react-chatbot-kit"
  }
}
